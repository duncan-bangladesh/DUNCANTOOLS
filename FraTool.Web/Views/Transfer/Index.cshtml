@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.18.5/xlsx.full.min.js"></script>
<style>
    table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
        padding: 6px;
    }
</style>
<div class="row rowtop">
    <div class="col-md-12">
        <div class="card card-info card-outline">
            <div class="card-header">
                <div class="card-tools">
                    <label class="control-label">Transfer</label>
                </div>
            </div>
            <div class="card-body">
                <form class="form-horizontal">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group row">
                                <label class="control-label text-md-right text-sm-left col-md-4">Company</label>
                                <div class="col-md-8">
                                    <select id="CompanyId" tabindex="2" class="form-control select2 select2-hidden-accessible" style="width: 100%;" data-select2-id="2" aria-hidden="true">
                                        <option value="0">--Select--</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="control-label text-md-right text-sm-left col-md-4">Upload Excel</label>
                                <div class="col-md-8">
                                    <input type="file" id="excelFile" disabled = "disabled" accept=".xlsx, .xls" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <div id="displayUploadedData">
                    <hr />
                    <table id="excelDataTable" class="table table-bordered table-hover table-sm">
                        <thead>
                            <tr style="background-color: #ebebe0;">
                                <th>Year</th>
                                <th>Month</th>
                                <th>Account No</th>
                                <th>Description</th>
                                <th>Crop</th>
                                <th>Amount</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                    <hr />
                    <form class="form-horizontal">
                        <div class="row">
                            <div class="form-group">
                                <div class="col-md-12">
                                    <input type="button" tabindex="13" id="btnSave" value="Save" class="btn btn-success" />
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

@section Scripts {
    <script src="~/js/transfer/transfer.js"></script>
    <script></script>
}